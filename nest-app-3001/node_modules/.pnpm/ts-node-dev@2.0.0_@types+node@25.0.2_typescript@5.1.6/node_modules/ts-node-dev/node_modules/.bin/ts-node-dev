#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@25.0.2_typescript@5.1.6/node_modules/ts-node-dev/lib/node_modules:/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@25.0.2_typescript@5.1.6/node_modules/ts-node-dev/node_modules:/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@25.0.2_typescript@5.1.6/node_modules:/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@25.0.2_typescript@5.1.6/node_modules/ts-node-dev/lib/node_modules:/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@25.0.2_typescript@5.1.6/node_modules/ts-node-dev/node_modules:/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@25.0.2_typescript@5.1.6/node_modules:/home/atin/Dev_node/Nest_js_2/nest-app-3001/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
